{
  "dotenv.enableAutocloaking": false,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [
    120
  ],
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.encoding": "utf8",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "peacock.color": "#3399cc",
  "search.exclude": {
    ".cache/**": true,
    "build/**": true,
    "node_modules/**": true,
    "public/build/**": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#5cadd6",
    "activityBar.background": "#5cadd6",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f9e6f2",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#5cadd6",
    "statusBar.background": "#3399cc",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#297aa3",
    "statusBarItem.remoteBackground": "#3399cc",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#3399cc",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#3399cc99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "editor.formatOnSave": true
}
